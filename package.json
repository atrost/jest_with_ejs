{
  "name": "jest_with_ejs",
  "version": "1.0.0",
  "description": "a repro repo for jest with backbone and lodash templating ",
  "main": "index.js",
  "author": "<atrost@marketron.com>",
  "license": "MIT",
  "scripts": {
    "jest:test": "jest --watch",
    "jest:coverage": "jest --coverage"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "ejs-loader": "^0.3.1",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "babel-jest": "^22.4.3",
    "jest": "^22.4.3"
  },
  "jest": {
    "verbose": true,
    "testRegex": "test.js$",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "ejs",
      "jsx"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ejs$": "<rootDir>/tools/ejs_tmpl_transformer.js"
    }
  }
}
